<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h4>ジャンル編集</h4>
      <%= form_with model: @genre, url: admin_genre_path do |f| %>
        <label>ジャンル名</label>
        <%= f.text_field :name %>
        <%= f.submit '変更を保存', class: 'btn btn-success' %>
      <% end %>
    </div>
    <div class="col-md-6"></div>
  </div>
</div>